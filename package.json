{
  "name": "wil-virtualized",
  "version": "1.0.0",
  "main": "./lib/index.js",
  "scripts": {
    "dev": "webpack-dev-server --mode=development",
    "lib": "rm -rf lib && cross-env NODE_ENV=production babel src --out-dir lib --copy-files --ignore __tests__,spec.js,test.js,__snapshots__",
    "flow-copy": "flow-copy-source src lib",
    "build": "webpack --mode=production",
    "flow": "flow",
    "flow-stop": "flow stop",
    "lint": "eslint src",
    "test-all": "yarn lint && yarn flow && yarn build",
    "release": "yarn lint && yarn flow && release-it"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint src --fix",
      "git add"
    ],
    "{*.json,.{babelrc,eslintrc,prettierrc,stylelintrc}}": [
      "prettier --ignore-path .eslintignore --parser json --write",
      "git add"
    ],
    "*.{css,scss}": [
      "stylelint --ignore-path .eslintignore --syntax scss --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && flow check"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/plugin-transform-flow-strip-types": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-0": "^7.0.0",
    "@babel/runtime": "^7.4.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-flowtype": "^3.6.1",
    "eslint-plugin-import": "^2.17.1",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "flow-bin": "^0.98.1",
    "flow-copy-source": "^2.0.6",
    "gh-pages": "^2.0.1",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.0.1",
    "react-test-renderer": "^16.8.6",
    "release-it": "^12.2.1",
    "stylelint": "^15.10.1",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-standard": "^18.3.0",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.6.0",
    "write-file-webpack-plugin": "^4.5.0",
    "yarn": "^1.16.0"
  },
  "description": "Validate react form",
  "files": [
    "README.md",
    "lib"
  ],
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wiloke1/wilvalidator.git"
  },
  "keywords": [
    "state",
    "management"
  ],
  "author": "wiloke",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wiloke1/wilvalidator/issues"
  },
  "homepage": "https://github.com/wiloke1/wilvalidator#readme"
}
